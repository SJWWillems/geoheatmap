Package: geoheatmap
Type: Package
Title: Create Geospatial Cartogram Heatmaps
Version: 0.1.0
Authors@R: c(
    person("Sanne", "Willems", email = "s.j.w.willems@fsw.leidenuniv.nl", role = c("aut", "cre")),
